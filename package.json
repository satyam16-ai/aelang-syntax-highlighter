{
  "name": "aelang-language-support",
  "displayName": "ÆLang Language Support",
  "description": "Complete syntax highlighting for ÆLang with universal type support (i8-i64, u8-u64, f8-f64, num, bool, char) and format specifiers",
  "version": "0.2.0",
  "publisher": "satyam",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "aelang",
    "systems programming",
    "os development",
    "compiler",
    "syntax highlighting"
  ],
  "contributes": {
    "languages": [
      {
        "id": "aelang",
        "aliases": [
          "ÆLang",
          "AELang",
          "aelang"
        ],
        "extensions": [
          ".ae"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "aelang",
        "scopeName": "source.aelang",
        "path": "./syntaxes/aelang.tmLanguage.json"
      }
    ],
    "iconThemes": [
      {
        "id": "aelang-icons",
        "displayName": "ÆLang Icons",
        "path": "./aelang-icon-theme.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/satyam/aelang-vscode-extension.git"
  },
  "bugs": {
    "url": "https://github.com/satyam/aelang-vscode-extension/issues"
  },
  "homepage": "https://github.com/satyam/aelang-vscode-extension#readme",
  "dependencies": {
    "vsce": "^2.15.0"
  }
}
