{
  "name": "aelang-language-support",
  "displayName": "ÆLang Language Support",
  "description": "Syntax highlighting and language support for ÆLang OS development language",
  "version": "0.0.1",
  "publisher": "satyam",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "aelang",
    "systems programming",
    "os development",
    "compiler",
    "syntax highlighting"
  ],
  "contributes": {
    "languages": [{
      "id": "aelang",
      "aliases": ["ÆLang", "AELang", "aelang"],
      "extensions": [".ae"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./icon.png",
        "dark": "./icon.png"
      }
    }],
    "grammars": [{
      "language": "aelang",
      "scopeName": "source.aelang",
      "path": "./syntaxes/aelang.tmLanguage.json"
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/satyam/aelang-vscode-extension.git"
  },
  "bugs": {
    "url": "https://github.com/satyam/aelang-vscode-extension/issues"
  },
  "homepage": "https://github.com/satyam/aelang-vscode-extension#readme"
}
